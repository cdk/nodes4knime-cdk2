   	<extension point="org.knime.workbench.repository.categories">
       <category after="" description="Nodes using the Chemistry Development Kit (CDK)" icon="icons/cdk.png" level-id="cdk2" name="CDK 2" path="/community"/>
       <category description="Nodes for CDK molecule conversion" icon="icons/category_translator.png" level-id="translators" name="Translators" path="/community/cdk2"/>
       <category description="Nodes for CDK molecule conversion" icon="icons/category_io.png" level-id="io" name="I/O" path="/community/cdk2"/>
       <category description="Nodes for 3D CDK molecules" icon="icons/category_3d.png" level-id="3d" name="3D" path="/community/cdk2"/>
       <category description="AMBIT" icon="icons/category_ambit.png" level-id="ambit" name="AMBIT" path="/community/cdk2"/>
   </extension>
   <extension point="org.eclipse.equinox.preferences.preferences">
      <initializer class="org.openscience.cdk.knime.preferences.CDKPreferenceInitializer"/>
   </extension>
   <extension point="org.eclipse.ui.preferencePages">
      <page category="org.knime.chem.preferences" class="org.openscience.cdk.knime.preferences.CDKPreferencePage" id="org.knime.ext.chem.cdk.preferences" name="CDK"/>
   </extension>
   <extension point="org.knime.product.splashExtension">
		<splashExtension icon="icons/cdk_splash_48.png" icon24="icons/cdk_splash_24.png" icon32="icons/cdk_splash_32.png" id="cdk" tooltip="CDK">
		</splashExtension>
   </extension>
   <extension point="org.knime.core.DataValueRenderer"> 
	 <dataValue valueClass="org.openscience.cdk.knime.type.CDKValue"> 
		 <renderer rendererFactoryClass="org.openscience.cdk.knime.type.CDKValueRenderer$Factory" suggestAsDefault="false"> 
		 </renderer> 
	 </dataValue> 
</extension>
<extension point="org.knime.core.DataType">
      <DataType cellClass="org.openscience.cdk.knime.type.CDKCell3">
         <serializer cellClass="org.openscience.cdk.knime.type.CDKCell" serializerClass="org.openscience.cdk.knime.type.CDKCell$CDKSerializer">
         </serializer>
         <serializer cellClass="org.openscience.cdk.knime.type.CDKAdapterCell" serializerClass="org.openscience.cdk.knime.type.CDKAdapterCell$Serializer">
         </serializer>
         <serializer cellClass="org.openscience.cdk.knime.type.CDKCell2" serializerClass="org.openscience.cdk.knime.type.CDKCell2$CDKSerializer">
         </serializer>
         <serializer cellClass="org.openscience.cdk.knime.type.CDKCell3" serializerClass="org.openscience.cdk.knime.type.CDKCell3$CDKSerializer">
         </serializer>
      </DataType>
   </extension>
<extension
      point="org.knime.core.DataCellToJavaConverter">
   <DataCellToJavaConverter
         factoryClass="org.openscience.cdk.knime.type.snippets.java.CdkCellToJavaFactory">
   </DataCellToJavaConverter>
</extension>
<extension
      point="org.knime.core.JavaToDataCellConverter">
   <JavaToDataCellConverter
         factoryClass="org.openscience.cdk.knime.type.snippets.java.JavaToCdkCellFactory">
   </JavaToDataCellConverter>
</extension>
</plugin>
